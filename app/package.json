{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --report-unused-disable-directives --max-warnings 0 --fix src",
    "preview": "vite preview",
    "create-changelog": "auto-changelog -p --template keepachangelog -u --tag-pattern .+ && git add CHANGELOG.md && git commit -m 'chore: update changelog' --no-verify || true",
    "dev": "NODE_ENV=development vite",
    "build": "NODE_ENV=production rm -rf dist && vite build",
    "build:push": "run-p create-changelog build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:dependencies": "depcheck",
    "test:ci": "run-p test test:dependencies",
    "install-logger": "cp node_modules/@mv-d/toolbelt/dist/logger.js public/logger.js",
    "postinstall": "pnpm install-logger && husky install",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "build-storybook": "storybook build"
  },
